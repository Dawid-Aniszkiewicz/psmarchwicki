int n,m,d;
scanf("%d%d",&n,&m);
while(n>0&&m>0){
    if(n>m){
        n=n%m;
    }else m=m%n;
    if(n==0){
        d=m;
    }else d=n;
    printf("nwd(%d,%d)=%d",n,m,d);
}
}

